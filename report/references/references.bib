@article{baker_picking_2015,
	title = {Picking the right cherries? {A} comparison of corpus-based and qualitative analyses of news articles about masculinity},
	url = {http://dcm.sagepub.com/content/early/2015/02/02/1750481314568542.abstract},
	doi = {10.1177/1750481314568542},
	abstract = {As a way of comparing qualitative and quantitative approaches to critical discourse analysis (CDA), two a®nalysts independently examined similar datasets of newspaper articles in order to address the research question ‘How are different types of men represented in the British press?’. One analyst used a 41.5 million word corpus of articles, while the other focused on a down-sampled set of 51 articles from the same corpus. The two ensuing research reports were then critically compared in order to elicit shared and unique findings and to highlight strengths and weaknesses between the two approaches. This article concludes that an effective form of CDA would be one where different forms of researcher expertise are carried out as separate components of a larger project, then combined as a way of triangulation.},

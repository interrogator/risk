@article{virtanen_definitions_1990,
	title = {{ON} {THE} {DEFINITIONS} {OF} {TEXT} {AND} {DISCOURSE}.},
	volume = {24},
	issn = {01654004},
	url = {https://search.ebscohost.com/login.aspx?direct=true&db=a9h&AN=14118712&site=ehost-live},
	abstract = {The aim of the paper is to trace the background of the various uses of the terms text and discourse in linguistic study. The use of two separate terms, text and discourse, may often be related to two different but complementary perspectives on language. A text may be viewed as a structure and/or it may be regarded as a process. In line with these two approaches, text is often used of a static concept — the product of a process — while discourse is used to refer to a dynamic notion — the process of text production and comprehension. It is the processual approach that naturally takes situational context into account. In addition to these two approaches to the study of language, a conceptual analysis also reveals that the two terms have gradually come closer to each other, as a result of the expansion of the scope of the notion text. A comparison between different uses of text shows a development from a descriptive structural concept, that of a supra-sentential unit, towards a more processual concept that easily incorporates situational factors as well. The development of the notion towards a more processual concept goes hand-in-hand with the growing emphasis on processes in text and discourse linguistics today. [ABSTRACT FROM AUTHOR]},
	number = {3/4},
	journal = {Folia Linguistica},
	author = {Virtanen, Tuija},
	month = jul,
	year = {1990},
	note = {Cited by 0011},
	keywords = {DEFINITIONS, LANGUAGE \& languages, LEXICOLOGY, LINGUISTICS, TERMS \& phrases},
	pages = {447--455}
}


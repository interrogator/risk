	keywords = {Depressive Disorder, Depressive Disorder, Major, Education, Medical, Continuing, General Practice, General Practitioners, Humans, Hungary, Physician's Role, Psychotherapy, Social Support, Suicidal Ideation, suicide, Suicide, Attempted},
	pages = {245--251}

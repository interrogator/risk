	journal = {BMC psychiatry},
	author = {Kluge, Michael and Hegerl, Ulrich and Sander, Christian and Dietzel, Jens and Mergl, Roland and Bitter, Istvan and Demyttenaere, Koen and Gusmão, Ricardo and Gonzalez-Pinto, Ana and Perez-Sola, Victor and Vieta, Eduard and Juckel, Georg and Zimmermann, Ulrich S and Bauer, Michael and Sienaert, Pascal and Quintão, Sónia and Edel, Marc-Andreas and Bolyos, Csilla and Ayuso-Mateos, Jose Luis and López-García, Pilar},
